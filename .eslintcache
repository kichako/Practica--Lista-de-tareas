pletada){\r\n                    return <Tarea\r\n                                key={tarea.id}\r\n                                tarea={tarea}\r\n                                toggleCompletada={toggleCompletada}\r\n                                editarTarea={editarTarea}\r\n                                borrarTarea={borrarTarea}\r\n                    />\r\n                }\r\n                // Si ya esta completada no la devolvemos.\r\n                return;\r\n            })\r\n            :\r\n            <div className=\"lista-tareas__mensaje\">\r\n               <p>~ No hay tareas agregadas...</p>\r\n            </div>\r\n            }\r\n        </ul>\r\n    );\r\n}\r\n \r\nexport default ListaTareas;","C:\\Users\\Kicha\\Desktop\\Cursos - Udemy\\React\\practicas\\app_lista_tareas\\src\\components\\Tarea.js",[],{"ruleId":"32","replacedBy":"33"},{"ruleId":"34","replacedBy":"35"},{"ruleId":"36","severity":1,"message":"37","line":28,"column":13,"nodeType":"38","messageId":"39","endLine":28,"endColumn":20},{"ruleId":"36","severity":1,"message":"40","line":54,"column":17,"nodeType":"38","messageId":"39","endLine":54,"endColumn":24},"no-native-reassign",["41"],"no-negated-in-lhs",["42"],"array-callback-return","Array.prototype.filter() expects a return value from arrow function.","ReturnStatement","expectedReturnValue","Array.prototype.map() expects a return value from arrow function.","no-global-assign","no-unsafe-negation"]